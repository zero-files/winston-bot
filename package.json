    "task:commander": "cross-env TASK=COMMANDER npm run start:dev"
  },
  "homepage": "https://github.com/zero-files/winston-bot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zero-files/winston-bot.git"
  },
  "bugs": {
    "url": "https://github.com/zero-files/winston-bot/issues"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
