{
  "name": "winston-bot",
  "version": "0.0.2",
  "description": "Winston Bot es un bot basado en el bot Winston de Mibbit ",
  "main": "./dist/main.js",
  "author": "Zero Files",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "node .",
    "start:dev": "cross-env ENVIRONMENT=local nest start --watch",
    "test": "cross-env ENVIRONMENT=test jest --config jest.config.ts",
